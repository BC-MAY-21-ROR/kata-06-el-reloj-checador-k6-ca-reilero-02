<%= form_with model: @company do |form| %>
  <% @company.errors.full_messages.each do |error|%>
    <div>
      <%= error %>
    </div>
  <%end%>
  <div>
    <%= form.label :name%>
    <%= form.text_field :name%>
  </div>

  <div>
    <%= form.label :address%>
    <%= form.text_area :address%>
  </div>
  <%= form.submit%>
<%= end %>